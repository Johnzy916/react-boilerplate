{
  "presets": [
    ["@babel/preset-env", {
      useBuiltIns: "usage",
      corejs: {
        version: 3.2,
        proposals: true
      }
    }],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-transform-runtime",
    [
      "@babel/transform-react-jsx",
      {
        "pragma": "React.createElement"
      }
    ]
  ]
}